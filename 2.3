public class ex2 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        String n = in.next();

        int res = 0;

        try {
            for(int i = 0; i <= n.length()-1; i++){
               // int d = n.substring(i, i + 1).codePointAt(0);
                if (Character.isDigit(n.substring(i, i + 1).codePointAt(0)))
                    res += Integer.parseInt(n.substring(i, i + 1));
            }
            System.out.println("Sum: " + res);

        }
        catch (Exception e){
            System.out.println("Error! Introduced word expression");
        }

    }
}
