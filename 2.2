package com.company;

import java.util.Arrays;
import java.util.Scanner;
import java.util.Random;

public class ex1 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        int amountTwo = 0;
        int amountThree = 0;
        int amountFour = 0;

        int n = in.nextInt();

        int[] arr = new int[n];

        for(int i = 0; i < arr.length; i++){
            arr[i] = (int) (Math.random() * (1000- - 0) + 0);
            System.out.printf("%d ", arr[i]);
        }

        for (int i : arr){
            if(i <= 99 && i >= 10){
                amountTwo++;
            }
            else if(i <= 999 && i > 100){
                amountThree++;
            }
            else if(i <= 9999 && i > 1000){
                amountFour++;
            }
        }
        System.out.println();
        System.out.println("Двухзначные: " + amountTwo);
        System.out.println("Трёхзначные: " + amountThree);
        System.out.println("Четырёхзначные: " + amountFour);

    }
}

